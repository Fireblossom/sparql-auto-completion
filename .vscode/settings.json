{
	"editor.insertSpaces": false,
	"tslint.enable": true,
	"typescript.tsc.autoDetect": "off",
	"typescript.preferences.quoteStyle": "single",
	"editor.codeActionsOnSave": {
		"source.fixAll.eslint": true
	},
	"grammarly.overrides": [
		{
			"files": [
				"**/README.md"
			],
			"config": {
				"audience": "expert",
				"dialect": "american",
				"domain": "general",
				"emotions": [],
				"goals": [
					"inform",
					"describe"
				]
			}
		}
	]
}